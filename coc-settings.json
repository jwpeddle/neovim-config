{
  "languageserver": {
    "python": {
      "command": "python",
      "filetypes": [
        "python"
      ],
      "settings": {
        "pyls": {
          "builtin.installExtrasArgs": [
            "flake8"
          ],
          "plugins": {
            "pycodestyle.enabled": false
          }
        }
      }
    }
  }
}
